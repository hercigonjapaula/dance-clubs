@model IEnumerable<DanceClubs.Data.Models.GroupUser>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Korisničko ime
            </th>
            <th>
                Grupa
            </th>
            <th>
                Uloga
            </th>
            <th>
                Član od
            </th>
            <th>
                Član do
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ApplicationUser.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Group.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GroupRole.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MemberFrom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MemberTo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
