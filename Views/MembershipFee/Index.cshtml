@model DanceClubs.Models.ClubIndexModel

@{
    ViewData["Title"] = "Create";
}

<h1>Dodaj</h1>

<h4>Slika</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <select id="Klub" asp-items="Model.ClubList.Select(i=>new SelectListItem(i.Name,i.Id.ToString()))"></select>
            </div>

        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Lista slika</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
