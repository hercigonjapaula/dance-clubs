@model DanceClubs.Models.ClubIndexModel


<div style="float:right; padding:15px">
    <a class="btn btn-primary" href="/Clubs/Create">Dodaj klub</a>
</div>
<table class="table table-hover" id="clubIndexTable">
    <tbody style="background-color:white">
        @foreach (var club in Model.ClubList)
        {
            <tr>
                <td>
                    <center>@club.Name</center>
                </td>
            </tr>            
        }
    </tbody>
</table>